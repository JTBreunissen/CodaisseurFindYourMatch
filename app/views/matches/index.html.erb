<div class="container">
  <div class="row">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3>Your Matches</h3>
      </div>
      <div class="panel-body">
        <div class="container">
          <div class="row">
            <% @studentmatches.each do |match|  %>
              <div class="col-md-7">
                <%= match.day %> | <%= match.first_student.name %> & <%= match.second_student.name %> <br>
              </div>
            <% end %>
         </div>
       </div>
     </div>
   </div>
 </div>
</div>
